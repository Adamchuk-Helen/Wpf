<Window x:Class="SocketClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SocketClient"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="400">
    <StackPanel>
        <Label Margin="10" Content="Введіть індекс від 10001 до 10061"/>
        <TextBox Margin="10" x:Name="text"/>
        <Button  Width="150" Background="GreenYellow" Content="Відправити індекс"  Click="BtClick">
            </Button>
        <Label Content="Вулиці:"/>
        
        <ListBox  Height="289">

            <ListBoxItem x:Name="res">
            </ListBoxItem>
        </ListBox>

    </StackPanel>
</Window>
